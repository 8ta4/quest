{
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["js/answer.js"],
      "matches": ["<all_urls>"],
      "exclude_matches": ["https://8ta4.github.io/quest/public/question.html"]
    },
    {
      "js": ["js/question.js"],
      "matches": ["https://8ta4.github.io/quest/public/question.html"]
    }
  ],
  "manifest_version": 3,
  "name": "quest",
  "permissions": ["webNavigation"],
  "version": "0.2.0"
}
